android_app {
    name: "MinimalBinder",
    manifest: "AndroidManifest.xml",
    srcs: [
        "src/**/*.kt",
        "src/**/I*.aidl"
    ],
    platform_apis: true,
    certificate: "platform",
    privileged: true,
    system_ext_specific: true,
    optimize: {
        enabled: false,
    },
}